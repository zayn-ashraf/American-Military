{\cs62\lang1033\langfe1033\b\i\ul0\strike0\scaps0\fs17\afs17\charscalex100\expndtw0\cf1\dn0 April 5}
\par
\pard\plain\li20\ri20\qj\sl-226\slmult0\pvpg\phpg\posx3715\posy2350\absh-10564\absw6586\overlay\s116\aspalpha\aspnum\faauto\adjustright
