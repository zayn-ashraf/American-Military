{\cs62\lang1033\langfe1033\b\i\ul0\strike0\scaps0\fs17\afs17\charscalex100\expndtw0\cf1\dn0 December 3}
\par
\pard\plain\li20\ri20\qj\sl-226\slmult0\pvpg\phpg\posx3666\posy2319\absh-10794\absw6581\overlay\s116\aspalpha\aspnum\faauto\adjustright
