{\cs62\lang1033\langfe1033\b\i\ul0\strike0\scaps0\fs17\afs17\charscalex100\expndtw0\cf1\dn0 November 25}
\par
\pard\plain\li20\ri20\qj\sl-226\slmult0\pvpg\phpg\posx1991\posy2338\absh-10766\absw6581\overlay\s116\aspalpha\aspnum\faauto\adjustright
